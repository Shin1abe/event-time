generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model GroceryList {
  id      Int      @id @default(autoincrement())
  title   String
  checked Boolean? @default(false)
}

model etList {
  id      Int      @id @default(autoincrement())
  title   String
  checked Boolean? @default(false)
}


model etEvent {
  eventId Int  @id @default(autoincrement())
  eventName String
  eventUrl String
  eventMemo String?
}